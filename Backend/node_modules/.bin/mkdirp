#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/nodejs/sessions/Introduction-to-Node-JS-Part-2/Samplelogin/Backend/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/cmd.js/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS-Part-2/Samplelogin/Backend/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/bin/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS-Part-2/Samplelogin/Backend/node_modules/.pnpm/mkdirp@1.0.4/node_modules/mkdirp/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS-Part-2/Samplelogin/Backend/node_modules/.pnpm/mkdirp@1.0.4/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS-Part-2/Samplelogin/Backend/node_modules/.pnpm/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS-Part-2/Samplelogin/Backend/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS-Part-2/Samplelogin/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS-Part-2/node_modules:/home/workspace/nodejs/sessions/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS-Part-2/Samplelogin/Backend/node_modules/mkdirp/bin/cmd.js/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS-Part-2/Samplelogin/Backend/node_modules/mkdirp/bin/node_modules:/home/workspace/nodejs/sessions/Introduction-to-Node-JS-Part-2/Samplelogin/Backend/node_modules/mkdirp/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkdirp/bin/cmd.js" "$@"
else 
  exec node  "$basedir/../mkdirp/bin/cmd.js" "$@"
fi
